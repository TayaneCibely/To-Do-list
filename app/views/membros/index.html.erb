<p id="notice"><%= notice %></p>

<h1>Membros</h1>

<table>
  <thead>
    <tr>
      <th>Email:</th>
      <th>Nome:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @membros.each do |membro| %>
      <tr>
        <td><%= membro.email %></td>
        <td><%= membro.nome %></td>
        <td><%= link_to 'Mostrar ', membro %></td>
        <td><%= link_to 'Editar ', edit_membro_path(membro) %></td>
        <td><%= link_to 'Apagar', membro, method: :delete, data: { confirm: 'VocÃª tem certeza que deseja apagar?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Membro', new_membro_path %>
